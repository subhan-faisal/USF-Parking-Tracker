@{
    ViewData["Title"] = "About Us";
}

<div class="container">
    <h2 class="my-4">@ViewData["Title"]</h2>
    
    <div class="card shadow mb-4">
        <div class="card-body">
            <div class="row">
                <div class="col-md-3 text-center mb-4">
                    <img src="~/images/team/subhan.jpg" 
                         alt="Subhan Faisal" 
                         class="rounded-circle img-fluid mb-3" 
                         style="max-width: 150px;">
                    <h5>Subhan Faisal</h5>
                    <p class="text-muted">UI/UX Designer</p>
                    <p class="small">Responsive Layout & Styling, API and Azure deployment.</p>
                </div>
                
                <div class="col-md-3 text-center mb-4">
                    <img src="~/images/team/jennifer.jpg" 
                         alt="Jennifer Negron" 
                         class="rounded-circle img-fluid mb-3" 
                         style="max-width: 150px;">
                    <h5>Jennifer Negron</h5>
                    <p class="text-muted">Project Lead</p>
                    <p class="small">Home Page and CRUD Navigation.</p>
                </div>
                
                <div class="col-md-3 text-center mb-4">
                    <img src="~/images/team/ronia.jpg" 
                         alt="Ronia Arabian" 
                         class="rounded-circle img-fluid mb-3" 
                         style="max-width: 150px;">
                    <h5>Ronia Arabian</h5>
                    <p class="text-muted">Data Viz Specialist</p>
                    <p class="small">Responsible for backend architecture and Azure deployment.</p>
                </div>
                
                <div class="col-md-3 text-center mb-4">
                    <img src="~/images/team/chandar.jpg" 
                         alt="Chandar Rathala" 
                         class="rounded-circle img-fluid mb-3" 
                         style="max-width: 150px;">
                    <h5>Chandar Rathala</h5>
                    <p class="text-muted">Backend Architect</p>
                    <p class="small">Responsible for OpenMap API integration and data processing.</p>
                </div>
            </div>
        </div>
    </div>
    
    <!-- Rest of your code remains the same -->
    <div class="row">
        <div class="col-md-6 mb-4">
            <!-- Technical Details card content -->
        </div>
        
        <div class="col-md-6 mb-4">
            <!-- Data Model card content -->
        </div>
    </div>
</div>